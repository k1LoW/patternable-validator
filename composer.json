{
    "name": "k1low/patternable-validator",
    "description": "PatternableValidator plugin for CakePHP 3",
    "keywords": ["validation"],
    "license": "MIT",
    "authors": [
        {
            "name": "Ken'ichiro Oyama",
            "homepage": "https://github.com/k1LoW",
            "role": "Author"
        }
    ],
    "type": "cakephp-plugin",
    "require": {
        "php": ">=5.4.16",
        "cakephp/cakephp": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
